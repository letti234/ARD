//printing

print('Hello World!');

//square brackets make lists
var listo = [0, 1, 1, 2, 3, 5];
print('List of numbers:', listo);


//objects
var object = {
    foo: 'bar',
    baz: 13,
    stuff: ['this', 'that', 'the other']
};

print('dictionary', object);

print('dictionary', object['foo']);
print('dictionary', object.stuff)


//functions
var reflect = function(element) {
  // Return the argument.
  return element;
};  

print('A good day to you!', reflect('Get off of my lawn you crazy kids!'));

//Define string and put it in the ee container
var aString = 'to the cloud!';
var eeString = ee.String(aString);


//Visualize images and image bands ////////////////////

// Instantiate an image with the Image constructor.
var image = ee.Image('CGIAR/SRTM90_V4');
//SRTM is an elevation raster and is from the Earth Engine data catalog. 

// Zoom to a location.
Map.setCenter(-112.8598, 36.2841, 9); // Center on the Grand Canyon.

// Display the image on the map.
Map.addLayer(image);

//Find more information on the data type
print('SRTM image', image);
//Is number of bands number of raster layers?

//customize layer visualization: note that you can define new raster layers 
//with nice color palettes and etc. 
Map.addLayer(image, {min: 0, max: 3000}, 'custom visualization');

Map.addLayer(image, {min: 0, max: 3000, palette: ['blue', 'green', 'red']},
    'custom palette');


