//Sub-Saharan Africa administrative district shapefile
var Rwanda = ee.FeatureCollection('users/salem043/Africa_Districts')
          .filterMetadata('ADM0', 'equals', 'Rwanda');
print(Rwanda.limit(5));


//Camp dataset
var camps = ee.FeatureCollection('users/salem043/camp');
Map.addLayer(camps, {color: 'red'}, 'camps');
print(camps);

//Population data
