////////////////////////////////////////////////////////////////////
//
//  Get tile's country
//
////////////////////////////////////////////////////////////////////


//Specify the properties that I want to copy from the SSA shapefile
var properties = ["ADM0"];

// Define a spatial filter as geometries that intersect.
var spatialFilter = ee.Filter.intersects({
  leftField: '.geo',
  rightField: '.geo'
});

// Join the points and polygons and apply the spatial filter to keep only intersecting ones
var joinAll = ee.Join.saveAll('matched').apply(tiles, SSA, spatialFilter);

// Since the join adds intersecting polygons to a new property 'matched'
// we need to extract required info from the polygons and copy it to the points
var SSAdata = joinAll.map(function(feature){
  var joinedFeat =  ee.List(feature.get('matched'));
  var polygon = ee.Feature(ee.FeatureCollection(joinedFeat).first());
  return ee.Feature(feature.copyProperties(polygon, properties)).select(properties);
});



////////////////////////////////////////////////////////////////////
//
//  Count number of camps that fall within tile
//
////////////////////////////////////////////////////////////////////


var spatialFilter = ee.Filter.intersects({
  leftField: '.geo',
  rightField: '.geo',
  maxError: 10
});

var joined = ee.Join.saveAll({
  matchesKey: 'neighbors', 
}).apply({
  primary: tiles, 
  secondary: campRegion, 
  condition: spatialFilter
});

var inTileCamps = joined.map(function(pt) {
  var neighs = ee.List(pt.get('neighbors'));
  var filt = ee.Filter.bounds(tiles);
  pt=pt.set('camps', neighs.filter(filt).size());
  return(pt);
});

print("Tiles with camp counts", inTileCamps.limit(10));


////////////////////////////////////////////////////////////////////
//
//  Determine tile's biome
//
////////////////////////////////////////////////////////////////////


var ecodata = ee.FeatureCollection('RESOLVE/ECOREGIONS/2017');
//print('ecoregions', ecodata.limit(10));


//Specify the properties that I want to copy from the SSA shapefile
var properties = ["BIOME_NAME","ECO_NAME", "systemindex"];

// Define a spatial filter as geometries that intersect.
var spatialFilter = ee.Filter.intersects({
  leftField: '.geo',
  rightField: '.geo'
});

// Join the points and polygons and apply the spatial filter to keep only intersecting ones
var joinAll = ee.Join.saveAll('matched').apply(tiles, ecodata, spatialFilter);

// Since the join adds intersecting polygons to a new property 'matched'
// we need to extract required info from the polygons and copy it to the points
var ecosample = joinAll.map(function(feature){
  var joinedFeat =  ee.List(feature.get('matched'));
  var polygon = ee.Feature(ee.FeatureCollection(joinedFeat).first());
  return ee.Feature(feature.copyProperties(polygon, properties)).select(properties);
});

print(ecosample.limit(2), "biome");


////////////////////////////////////////////////////////////////////
//
//  Tile's mean night lights 2000-2012
//
////////////////////////////////////////////////////////////////////

/////Sum population counts in the pixels to the grid
///var Syr_gridpop = pop_Syria.reduceRegions(grid,ee.Reducer.sum());


//Save a subset of the lights data: just the stable lights band for 2013
var light2001 = lights.filterMetadata('system:index', 'equals', 'F152001');
print(light2001);

//Transform from image collection into simple image with one band
var lightImg2001 = light2001.toBands();
//print(lightsImage);

//Use reducer function to obtain mean lights per governorate
var lightTile2001 = lightImg2001.reduceRegions(tiles, ee.Reducer.mean());

var   light2002 = lights.filterMetadata('system:index', 'equals', 'F152002');
var lightImg2002 = light2002.toBands();
var lightTile2002 = lightImg2002.reduceRegions(lightTile2001, ee.Reducer.mean());


var   light2003 = lights.filterMetadata('system:index', 'equals', 'F152003');
var lightImg2003 = light2003.toBands();
var lightTile2003 = lightImg2003.reduceRegions(lightTile2002, ee.Reducer.mean());


var   light2004 = lights.filterMetadata('system:index', 'equals', 'F152004');
var lightImg2004 = light2004.toBands();
var lightTile2004 = lightImg2004.reduceRegions(lightTile2003, ee.Reducer.mean());

var   light2005 = lights.filterMetadata('system:index', 'equals', 'F152005');
var lightImg2005 = light2005.toBands();
var lightTile2005 = lightImg2005.reduceRegions(lightTile2004, ee.Reducer.mean());


var   light2006 = lights.filterMetadata('system:index', 'equals', 'F152006');
var lightImg2006 = light2006.toBands();
var lightTile2006 = lightImg2006.reduceRegions(lightTile2005, ee.Reducer.mean());


var   light2007 = lights.filterMetadata('system:index', 'equals', 'F152007');
var lightImg2007 = light2007.toBands();
var lightTile2007 = lightImg2007.reduceRegions(lightTile2006, ee.Reducer.mean());


var   light2008 = lights.filterMetadata('system:index', 'equals', 'F152008');
var lightImg2008 = light2008.toBands();
var lightTile2008 = lightImg2008.reduceRegions(lightTile2007, ee.Reducer.mean());


var   light2009 = lights.filterMetadata('system:index', 'equals', 'F162009');
var lightImg2009 = light2009.toBands();
var lightTile2009 = lightImg2009.reduceRegions(lightTile2008, ee.Reducer.mean());


var   light2010 = lights.filterMetadata('system:index', 'equals', 'F182010');
var lightImg2010 = light2010.toBands();
var lightTile2010 = lightImg2010.reduceRegions(lightTile2009, ee.Reducer.mean());


var   light2011 = lights.filterMetadata('system:index', 'equals', 'F182011');
var lightImg2011 = light2011.toBands();
var lightTile2011 = lightImg2011.reduceRegions(lightTile2010, ee.Reducer.mean());


var   light2012 = lights.filterMetadata('system:index', 'equals', 'F182012');
var lightImg2012 = light2012.toBands();
var lightTile2012 = lightImg2012.reduceRegions(lightTile2011, ee.Reducer.mean());

print("lights 2012", lightTile2012.limit(10));


////////////////////////////////////////////////////////////////////
//
//  Determine tile's buffer
//
////////////////////////////////////////////////////////////////////


var buffs2 = buffDissolveFC.select(["system:index"], ["Buffer ID"]);

var properties = ["Buffer ID"];

// Define a spatial filter as geometries that intersect.
var spatialFilter = ee.Filter.intersects({
  leftField: '.geo',
  rightField: '.geo'
});

// Join the points and polygons and apply the spatial filter to keep only intersecting ones
var joinAll = ee.Join.saveAll('matched').apply(tiles, buffs2, spatialFilter);

// Since the join adds intersecting polygons to a new property 'matched'
// we need to extract required info from the polygons and copy it to the points
var bufferID = joinAll.map(function(feature){
  var joinedFeat =  ee.List(feature.get('matched'));
  var polygon = ee.Feature(ee.FeatureCollection(joinedFeat).first());
  return ee.Feature(feature.copyProperties(polygon, properties)).select(properties);
});

print(bufferID.limit(2), "buffer ID");

