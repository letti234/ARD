Export.table.toDrive(
pointdata_withbandcounts_12, "lulcCampsGroup1", "deforTilesGEE");

//Export number of camps within tile
Export.table.toDrive(inTileCamps, "lulcCampsInTileGroup1", "deforTilesGEE");

//Export biome as csv
Export.table.toDrive(ecosample, "lulcBiomeGroup1", "deforTilesGEE");

//Export night lights as CSV
Export.table.toDrive(lightTile2012, "lulcLightsGroup1", "deforTilesGEE");

//Export buffer id as CSV
Export.table.toDrive(bufferID, "lulcBufferIDGroup1", "deforTilesGEE");

