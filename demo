
var region = ee.Geometry.Rectangle(-119.224, 34.669, -99.536, 50.064);

// Create 10 random points in the region.
var randomPoints = ee.FeatureCollection.randomPoints(region);

//Produce 10 km buffer this the are we are going to study
var bufferPoly = function(feature) {
  return feature.buffer(10000);  //!!Change back to 50k after making sure the border identification works 
};

var buffers = randomPoints.map(bufferPoly);
Map.addLayer(buffers, {}, 'buffers');


//var buffsSample=buffers.dissolve();
//Map.addLayer(buffsSample, {}, 'buffers dissolved');